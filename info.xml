<?xml version="1.0"?>
<extension key="com.joineryhq.inventoryfield" type="module">
  <file>inventoryfield</file>
  <name>Inventory Field</name>
  <description>Allows defining a custom 'select' field so that usage of select options is limited to once-per-event.</description>
  <license>GPL-3.0</license>
  <maintainer>
    <author>https://github.com/JoineryHQ/</author>
    <email>info@JoineryHQ.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/JoineryHQ/com.joineryhq.inventoryfield</url>
    <url desc="Documentation">https://github.com/JoineryHQ/com.joineryhq.inventoryfield</url>
    <url desc="Support">https://github.com/JoineryHQ/com.joineryhq.inventoryfield/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/gpl-3.0.html</url>
  </urls>
  <releaseDate>2021-05-20</releaseDate>
  <version>0.3</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>Developed by Joinery for FPPTA and the CiviCRM community.</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Inventoryfield</namespace>
  </civix>
</extension>
